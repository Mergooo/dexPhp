{
    "require": {
        "nikic/fast-route": "dev-master",
        "envms/fluentpdo": "dev-master",
        "twig/twig": "^3.0",
        "openai-php/client": "^1.0"
        },
        
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nikic/FastRoute"
        },
        {
            "type": "vcs",
            "url": "https://github.com/envms/fluentpdo"
        },
        {
            "type": "vcs",
            "url": "https://github.com/twigphp/Twig"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
